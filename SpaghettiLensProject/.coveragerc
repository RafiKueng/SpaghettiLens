

[run]

branch = True

source =
    SpaghettiLensProject
    LensesDB_app
    SpaghettiLens_app

#include =
#    SpaghettiLensProject/*
#    *_app/*

omit =
    manage.py
    SpaghettiLensProject/settings.py
    SpaghettiLensProject/settings/*
    */migrations/*


[report]

show_missing = True
skip_covered  = False  

exclude_lines =
    pragma: no cover
    
    # Don't complain about missing debug-only code:
    def __unicode__
    def __repr__
    if self\.debug
    
    # Don't complain if tests don't hit defensive assertion code:
    raise AssertionError
    raise NotImplementedError
    
    # Don't complain if non-runnable code isn't run:
    if 0:
    if __name__ == .__main__.:
    
